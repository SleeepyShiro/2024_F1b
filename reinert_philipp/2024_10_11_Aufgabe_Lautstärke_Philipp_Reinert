void setup() {
  // put your setup code here, to run once:
    pinMode(A0, INPUT);

    Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:
  Serial.println(analogRead(A0)); //AKTUEllen wert von Pin A0 ausgeben
  delay(80);
}

